g++ -g -c Scalar.cpp
g++ -g -o Scalar  Scalar.o  -lgfortran -lm -lblas -lslatec -llinpack -ldors -lseagods

g++ -g -c Vector.cpp
g++ -g -o Vector  Vector.o -lgfortran -lm -lblas -lslatec -llinpack -ldors -lseagods

g++ -g -o NormaliseS NormaliseS.cpp -lm -lblas -lslatec -ldors -llinpack -lseagods
g++ -g -o NormaliseV NormaliseV.cpp  -lm -lblas -lslatec -ldors -llinpack -lseagods

g++ -g -o View View.cpp -lgfortran -lm -lblas -lslatec -llinpack -ldors -lseagods
g++ -g -o ViewVec ViewVec.cpp -lgfortran  -lm -lblas -lslatec -llinpack -ldors -lseagods

