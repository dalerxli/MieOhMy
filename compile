g++ -g -o Scalar  Scalar.o $HOME/libs/mylib.a  $HOME/libs/libslatec.a $HOME/libs/libtoms.a  -lgfortran -lm


g++ -g -c Vector.cpp
g++ -g -o Vector  Vector.o $HOME/libs/mylib.a  $HOME/libs/libslatec.a $HOME/libs/libtoms.a  -lgfortran -lm

